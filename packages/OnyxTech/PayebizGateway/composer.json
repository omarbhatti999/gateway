{
    "name": "onyxtech/payebizgateway",
    "description": "A module for payment-gateways integration in Laravel",
    "license": "onyxtech2021",
    "authors": [
        {
            "name": "onyxtech",
            "email": "info@onyxtech.co.uk",
            "homepage": "www.onyxtech.co.uk"
        }
    ],
    "homepage": "https://github.com/omarbhatti999/payebizgateway",
    "keywords": ["Laravel", "PayebizGateway"],
    "require": {
